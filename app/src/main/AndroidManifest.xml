        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ProjectForAndroid311"
        tools:targetApi="31">
        <activity
            android:name=".ExampleForSharedPreferences"
            android:exported="false" />
        <activity
            android:name=".InternalStorageActivity"
            android:exported="false" />
        <activity
            android:name=".ExampleForApkFile"
            android:exported="false" />
        <activity
            android:name=".ExampleGalleryAndBaseAdapter"
            android:exported="false" />
        <activity
            android:name=".ExampleListView"
            android:exported="false" />
        <activity
            android:name=".ExampleHandlerThread"
            android:exported="false" />
        <activity
            android:name=".ExampleAsyncTask"
            android:exported="false" />
        <activity
            android:name=".ExampleMethoedsWaitNotifies"
            android:exported="false" />
        <activity
            android:name=".ExampleThreadPriority"
            android:exported="false" />
        <activity
            android:name=".SecondExampleThreads"
            android:exported="false" />
        <activity
            android:name=".ExampleThreads"
            android:exported="false" />
        <activity
            android:name=".ExampleForProgressBar"
            android:exported="false" />
        <activity
            android:name=".ExampleForToggleButton"
            android:exported="false" />
        <activity
            android:name=".ExampleForRadioButton"
            android:exported="false" />
        <activity
            android:name=".CalculatorActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <receiver
            android:name=".ApplicationClass$NotificationBroadcast"
            android:allowBackup="true"
            android:directBootAware="true"
            android:enabled="true"
            android:exported="false" />
    </application>

</manifest>